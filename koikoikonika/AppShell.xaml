<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="koikoikonika.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:koikoikonika"
    
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent Title="Dashboard"
                      ContentTemplate="{DataTemplate local:HomePage}"/>
        <ShellContent Title="Search"
                      ContentTemplate="{DataTemplate local:SearchPage}"/>
        <ShellContent Title="Settings"
                      ContentTemplate="{DataTemplate local:SettingPage}"/>
        <ShellContent Title="Student"
                      ContentTemplate="{DataTemplate local:StudentPage}"/>
    </TabBar>
        

</Shell>
